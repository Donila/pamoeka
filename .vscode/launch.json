{
        "version": "0.2.0",
        "configurations": [
            {
                "type": "node",
                "request": "launch",
                "name": "Launch Program",
                "program": "${workspaceRoot}/server/index.js"
            },
            {
                "type": "node",
                "request": "attach",
                "name": "Attach by Process ID",
                "processId": "${command:PickProcess}",
                "protocol": "legacy"
            }
        ]
    }